{
  "name": "dw-2",
  "version": "1.0.0",
  "description": "Set of apps for dumbways fullstack bootcamp stage 2. Using expressjs and typescript, and npm workspaces",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:blog": "npm run dev --workspace=apps/blog",
    "dev:store": "npm run dev --workspace=apps/store",
    "build:shared": "npm run build --workspace=packages/shared",
    "migrate:store": "npm run migrate --workspace=apps/store",
    "seed:store": "npm run seed --workspace=apps/store",
    "lint:blog": "npx eslint apps/blog",
    "lint:store": "npx eslint apps/store",
    "lint": "npx eslint ."
  },
  "keywords": [
    "typescript",
    "expressjs"
  ],
  "author": "mnabil1718",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "body-parser": "^2.2.1",
    "dotenv": "^17.2.3",
    "http-status-codes": "^2.3.0",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "prisma": "^7.2.0",
    "typescript-eslint": "^8.52.0"
  }
}
