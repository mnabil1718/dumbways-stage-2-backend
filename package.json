{
  "name": "dw-2",
  "version": "1.0.0",
  "description": "Set of apps for dumbways fullstack bootcamp stage 2. Using expressjs and typescript, and npm workspaces",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:blog": "npm run dev --workspace=apps/blog",
    "dev:store": "npm run dev --workspace=apps/store",
    "dev:supply": "npm run dev --workspace=apps/supply",
    "build:shared": "npm run build --workspace=packages/shared",
    "migrate:blog": "npm run migrate --workspace=apps/blog",
    "migrate:store": "npm run migrate --workspace=apps/store",
    "migrate:supply": "npm run migrate --workspace=apps/store",
    "seed:blog": "npm run seed --workspace=apps/blog",
    "seed:store": "npm run seed --workspace=apps/store",
    "seed:supply": "npm run seed --workspace=apps/supply",
    "db-refresh:blog": "npm run migrate:blog reset && npm run seed:blog",
    "db-refresh:store": "npm run migrate:store reset && npm run seed:store",
    "db-refresh:supply": "npm run migrate:supply reset && npm run seed:supply",
    "lint:blog": "npx eslint apps/blog",
    "lint:store": "npx eslint apps/store",
    "lint:supply": "npx eslint apps/supply",
    "lint:fix": "npx eslint . --fix",
    "lint": "npx eslint ."
  },
  "keywords": [
    "typescript",
    "expressjs"
  ],
  "author": "mnabil1718",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/cors": "^2.8.19",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "http-status-codes": "^2.3.0",
    "jose": "^6.1.3",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "prisma": "^7.2.0",
    "typescript-eslint": "^8.52.0"
  }
}
